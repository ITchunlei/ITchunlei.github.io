<div class="container-fluid">
    <div class="row">
        <div class="col-md-9">
            <div class="title">
                <h4>最新文章</h4>
            </div>
            <% page.posts.forEach(function(post) { %>
            <article>
                <h4><a href="<%- url_for(post.path) %>"><%- post.title%></a></h4>
                <span><%- date(post.date) %></span>
            </article>
            <% }) %>
        </div>
        <div class="col-md-3">
            right
        </div>
    </div>
</div>